{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/dist"
        }
      },
      {
        "src": "server/api/request-resume.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/client/dist/$1"
      }
    ],
    "cors": {
        "allowedOrigins": ["https://dev-v2-1.vercel.app"],
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "allowedHeaders": ["Content-Type", "Authorization"]
      }
  }
  